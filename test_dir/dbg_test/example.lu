#!/bin/lush

impure fn echo_impure (arg)
    echo $arg
end

fn echo_wrapped ( arg )
    echo $arg
end

let text = ["lush" "greets" "the world"]
echo_impure "LETS GO"

for txt in $text
    let x = $txt
    if $x == "lush"
        echo $x
    elif $x == "greets"
        echo_wrapped $x
    else
        echo $x
    end
end
