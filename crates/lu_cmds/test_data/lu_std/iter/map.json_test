map maps structs
===
use std:iter

struct Person{
    sallary: num
}

fn increase_sallary(arg: Person ret: Person)
    ret Person{
        sallary: $arg.sallary + 100
    }
end

let persons: [Person] = [(Person) [100] [200] [300]]

$persons | map $increase_sallary
